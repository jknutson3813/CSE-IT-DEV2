		<exclude>site-classic/*/*/*</exclude>
		<exclude>site-classic/*/*/*/*</exclude>
		<exclude>site-classic/*/*/*/*/*</exclude>
		<exclude>site-blank/*</exclude>
		<exclude>site-blank/*/*</exclude>
		<exclude>site-blank/*/*/*</exclude>
		<exclude>site-blank/*/*/*/*</exclude>
		<exclude>site-blank/*/*/*/*/*</exclude>
		<exclude>site-beginner/*</exclude>
